def removeDuplicates(nums):
    f not nums:
        return 0

    k = 1  # Start with the first element as unique
    for i in range(1, len(nums)):
        if nums[i] != nums[i - 1]:
            nums[k] = nums[i]
            k += 1

    return k


if __name__ == '__main__':
    nums = [1,2,3,1]
    removeDuplicates(nums)